{
	"name": "webpack-starters",
	"version": "1.0.0",
	"private": true,
	"homepage": "http://ali-kash.github.io/notes-app",
	"description": "Notes application using React, search through your notes, saves to localstorage, also has a 'dark mode'",
	"scripts": {
		"start": "cross-env SERVE=true webpack serve",
		"watch": "webpack --watch",
		"build": "cross-env NODE_ENV=production webpack",
		"build-dev": "webpack",
		"clean": "rm -rf ./dist",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist",
		"serve": "npm run build && serve dist"
	},
	"author": "Jimmy DC",
	"license": "ISC",

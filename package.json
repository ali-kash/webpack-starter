		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.0.1",
		"dotenv-webpack": "^7.0.3",
		"html-webpack-plugin": "^5.0.0-beta.5",
		"mini-css-extract-plugin": "^1.3.4",
		"postcss": "^8.3.9",
